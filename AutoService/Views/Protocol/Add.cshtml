        <link href="@Url.Content("~/css/circle.css")" rel="stylesheet" type="text/css" />*@
    <link href="@Url.Content("~/css/count-down.css")" rel="stylesheet" type="text/css" />
}
@section Scripts{
    <script type="text/javascript" src="~/js/countDown.min.js"></script>
    <script type="text/javascript">
        var timerInstance, skipChangeEventAvtomobilatENeIzpraven = false, skipChangeEventAvtomobilatEIzpraven = false;
        $(function () {
            $('.selectpicker').selectpicker({
                noneSelectedText: ''
            });
            $('.selectpicker').on('changed.bs.select', function (e, clickedIndex, isSelected, previousValue) {
                var selectValue = $(this).closest('.bootstrap-select').find('button .filter-option').html();
                $(this).closest('.bootstrap-select').parent().find('.hidden').val(selectValue);
            });

            $('#AvtomobilatEIzpraven').on('change', function () {
                if (!skipChangeEventAvtomobilatEIzpraven) {
                    skipChangeEventAvtomobilatENeIzpraven = true;
                    if (this.checked) {
                        $('#AvtomobilatENeIzpraven').attr('checked', 'checked');
                        $('.is-regular .btn-danger').addClass('active')

                    }
                    else {
                        $('#AvtomobilatENeIzpraven').removeAttr('checked');
                        $('.is-regular .btn-danger').removeClass('active')
                    }
                }
                else {
                    skipChangeEventAvtomobilatEIzpraven = false;
                }
            })
            $('#AvtomobilatENeIzpraven').on('change', function () {
                if (!skipChangeEventAvtomobilatENeIzpraven) {
                    skipChangeEventAvtomobilatEIzpraven = true;
                    if (this.checked) {
                        $('#AvtomobilatEIzpraven').attr('checked', 'checked');
                        $('.is-regular .btn-success').addClass('active')

                    }
                    else {
                        $('#AvtomobilatEIzpraven').removeAttr('checked');
                        $('.is-regular .btn-success').removeClass('active')
                    }
                }
                else {
                    skipChangeEventAvtomobilatENeIzpraven = false;
                }
            })
             $('a.incomplete').hide();
            $('#fade').show();
            $('[checked="checked"]').closest('.btn').addClass('active focus');

            @if (!Model.InLine) {
                    <text>
            var timerInstance = new Countdown({
                cont: document.querySelector('#waitingTimeTimer'),
                date: Date.now() + @ViewBag.WaitingTime * 60 * 1000,
                outputTranslation: {
                    minute: '',
                    second: '',
                },
                endCallback: function () {
                    closeProtokol();
                },
                outputFormat: 'minute|second',
            });
            timerInstance.start();
            $('#in-line').addClass('blocked');
            $('#fade-inline').show();
            </text>
                    }
            $('.in-line.btn-success').on('click', function () {
                timerInstance.stop();
                $('#waitingTimeTimer').hide();
                $('.inline-panel .disabled').show();
                $(this).closest('#in-line').removeClass('blocked');
                $('#fade-inline').hide();
            })
            $('.btn.add').on('click', function () {
                $(this).parent().find('.disabled').show();
            })
        })
        function closeProtokol() {
            $('#cancel').trigger('click');
        }
    </script>
}

