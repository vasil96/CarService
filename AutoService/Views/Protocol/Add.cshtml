                    </label><label asp-for="AvtomobilatEIzpraven">&nbsp;&nbsp;&nbsp;ИЗПРАВЕН</label>
                </div>
            </div>
            <br />
            <div class="form-group">
                <div class="col-md-5">
                    <label asp-for="Mehanik" class="control-label">Механик</label>
                </div>
                <div class="col-md-5">
                    <input asp-for="Mehanik" class="form-control" />
                </div>
            </div>
            <div class="form-group">
                <div class="col-md-5" data-toggle="buttons">
                    <label asp-for="IsBlock" class="btn btn-warning">
                        <input asp-for="IsBlock" />
                        <span class="glyphicon glyphicon-ok"></span>
                    </label><label asp-for="IsBlock">&nbsp;&nbsp;&nbsp;БЛОКИРАЙ МЯСТО</label>
                </div>
            </div>
        </div>
        <div class="form-group button-panel">
            <div class="col-md-3">
                <input asp-action="Add" type="submit" value="Запиши" class="btn btn-default add" name="add" />
                <div class="disabled" style="width:131px;"></div>
            </div>
            <div id="in-line" class="col-md-6">
                <div>
                    @if (!Model.InLine)
                    {
                        <div id="waitingTimeTimer">
                        </div>
                    }
                </div>

                <div class="inline-panel" data-toggle="buttons" style="position:relative;">
                    <label asp-for="InLine" class="btn btn-success in-line">
                        <input asp-for="InLine" />
                        <span class="glyphicon glyphicon-ok"></span>
                    </label><label asp-for="InLine">&nbsp;&nbsp;&nbsp;НА ЛИНИЯ</label>
                    <div class="disabled" @if (Model.InLine) { <text> style="display:block;" </text> }></div>
                </div>
            </div>
            <div class="col-md-3 right">
                <input asp-action="Cancel" type="submit" value="Откажи" class="btn btn-default btn-danger" name="cancel" id="cancel" />
            </div>
        </div>
        <div class="form-group">
            @Html.ValidationSummary()
        </div>
        @Html.AntiForgeryToken()
        <br />
        <br />
        <div id="fade-inline"></div>
    </form>
</div>



